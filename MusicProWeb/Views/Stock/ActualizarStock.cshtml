@model Stock

@{
    ViewBag.Title = "Actualizar Stock";

}
<h2 style="color: #FFFFFF">Actualizar Stock</h2>
<hr />

<form method="post" style="color: #FFFFFF" action="@Url.Action("ActualizarStock","Stock")">
    <div class="form-group" style="display: none" >
        <label>ID</label>
        @Html.TextBoxFor(x=> x.idStock, new { @class ="form-control", @readonly="readonly"})
     </div>
    <div class="form-group" >
        <label>ID Producto</label>
        @Html.TextBoxFor(x=> x.idProductostock, new { @class ="form-control"})
     </div>
     <div class="form-group" >
        <label>Cantidad del Producto</label>
        @Html.TextBoxFor(x=> x.total_prod, new { @class ="form-control"})
     </div>
     <div class="form-group" >
        <label>Bodega</label>
        @Html.TextBoxFor(x=> x.bodega, new { @class ="form-control"})
        </div>
     <hr />
     <div class="form-group" >
        <input type="submit" class="btn btn-success" value="Enviar" />
        <a class="btn btn-primary " href="@Url.Action("ListarStock","Stock")">Regresar</a>
     </div>

</form>